{
  "name": "js-offs",
  "version": "0.0.1",
  "description": "Owner Free File System",
  "main": "src/index.js",
  "appname": "offs",
  "scripts": {
    "compile-vue": "node ./build/compile-vue-templates.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "discard": "git reset --hard && git clean -f -d",
    "start": "npm run compile-vue && ./node_modules/.bin/electron .",
    "rebuild": "./node_modules/.bin/electron-rebuild",
    "build-native": "./node_modules/.bin/electron-packager ./",
    "build-linux-x64": "./node_modules/.bin/electron-packager ./ --platform=linux --arch=x64 --icon=src/electron/images/off-logo.png --asar=true --overwrite && zip -FS -r js-offs-linux-x64.zip js-offs-linux-x64",
    "build-linux-ia32": "./node_modules/.bin/electron-packager ./ --platform=linux --arch=ia32 --icon=src/electron/images/off-logo.png --asar=true --overwrite && zip -FS -r js-offs-linux-ia36.zip js-offs-linux-ia32",
    "build-windows-x64": "./node_modules/.bin/electron-packager ./ --platform=win32 --arch=x64 --icon=src/electron/images/off-logo.ico --asar=true --overwrite && zip -FS -r js-offs-win32-x64.zip js-offs-win32-x64",
    "build-windows-ia36": "./node_modules/.bin/electron-packager ./ --platform=win32 --arch=ia36 --icon=src/electron/images/off-logo.ico --asar=true --overwrite && zip -FS -r js-offs-win32-ia36.zip js-offs-win32-x64",
    "build-darwin-x64": "./node_modules/.bin/electron-packager ./ --platform=darwin --arch=x64 --icon=src/electron/images/off-logo.icns --asar=true --overwrite && zip -FS -r js-offs-darwin-x64.zip js-offs-darwin-x64"
  },
  "author": "Victor Morrow <vijayee.kulkaa@hushmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "block-stream2": "1.1.0",
    "bs58": "3.0.0",
    "buffer-split": "1.0.0",
    "buffer-xor": "2.0.1",
    "clipboard": "1.6.1",
    "clipboardy": "^1.2.3",
    "collect-stream": "1.1.1",
    "cron": "1.1.1",
    "cuckoo-filter": "1.0.3",
    "electron": "^1.8.7",
    "electron-ipc-responder": "^1.0.4",
    "electron-positioner": "3.0.0",
    "end-of-stream": "1.1.0",
    "express": "4.14.0",
    "get-folder-size": "1.0.0",
    "hamming-distance": "1.0.0",
    "increment-buffer": "1.0.0",
    "is-ip": "1.0.0",
    "isstream": "0.1.2",
    "js-logging": "0.1.0",
    "mime": "1.3.4",
    "mkdirp": "0.5.1",
    "nat-upnp": "1.0.2",
    "network": "0.2.1",
    "node-forge": "0.6.42",
    "pretty-bytes": "4.0.2",
    "protobufjs": "5.0.1",
    "protocol-buffers": "3.1.6",
    "readable-stream": "2.1.4",
    "streamifier": "0.1.1",
    "through2": "2.0.1",
    "timer-jobs": "0.1.0",
    "urldecode": "^1.0.1",
    "urlencode": "1.1.0",
    "vee-validate": "^2.0.9",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.5.0",
    "chai": "3.5.0",
    "electron-packager": "8.6.0",
    "electron-prebuilt": "1.4.13",
    "electron-rebuild": "1.5.6",
    "gulp": "3.9.1",
    "mocha": "3.1.2",
    "random-ipv4": "1.0.2",
    "vueify": "^9.4.1",
    "vuetable-2": "^1.7.5"
  }
